{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "STlink launch",
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/build/stm32f7-disco-template.elf",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "stutil",
			"device": "STM32F723",
			"interface": "swd",
			"runToMain": true, // else it starts at reset handler - not interested
			//"preLaunchTask": "Build all", // configured in tasks.json
			// "preLaunchCommands": ["Build all"], // you can execute command instead of task
			"svdFile": "${workspaceRoot}/STM32F723.svd", // Include svd to watch device peripherals
			"swoConfig": {} // currently (v1.7.0) not supported
		},
		{
			"name": "STlink attach",
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/build/stm32f7-disco-template.elf",
			"request": "attach",
			"type": "cortex-debug",
			"servertype": "stutil",
			"device": "STM32F723",
			"interface": "swd",
			//"runToMain": true, // else it starts at reset handler - not interested
			//"preLaunchTask": "Build all", // configured in tasks.json
			// "preLaunchCommands": ["Build all"], // you can execute command instead of task
			"svdFile": "${workspaceRoot}/STM32F723.svd", // Include svd to watch device peripherals
			"swoConfig": {} // currently (v1.7.0) not supported
		},
	]
}